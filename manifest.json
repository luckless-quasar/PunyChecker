{
    "manifest_version": 2,
    "name": "PunyChecker",
    "version": "0.01",
    "description": "Checks if the current domain uses Punycode",
    "icons": {
        "48": "icons/icon.svg",
        "96": "icons/icon.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "puny.js"
            ]
        }
    ]
}